		.br {border-right: 1px solid #e0e0e0;}




		.swipe {
			overflow: hidden;
			visibility: hidden;
			position: relative;
			background: #fff;
			padding-bottom: 10px;
		}
		.swipe-wrap {
			overflow: hidden;
			position: relative;
		}
