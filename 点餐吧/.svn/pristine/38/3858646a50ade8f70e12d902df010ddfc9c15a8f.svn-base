function gopay()
{
api.confirm({
	'title':'您的消费金额：'+totalcash,
	'msg':'是否开具发票？',
	'buttons':['是','否']
},function(ret,err){
if(ret.buttonIndex==1)
{
	api.prompt({
	'title':'发票抬头：',
    buttons: ['确定', '取消']
}, function(ret, err){
	if(ret.buttonIndex==1){
    var index = ret.buttonIndex;
    fapiao = ret.text;
    fukuan();
    }
});
}
else
{
	fukuan();
}
});
}
function fukuan()
{
totalcash=0;
	api.confirm({
	'title':'您的消费金额：'+totalcash,
	'msg':'是否在线支付？',
	'buttons':['是','否']
},function(ret,err){
if(ret.buttonIndex==1)
{
pay=1;
	api.openApp({
                            androidPkg: 'com.eg.android.AlipayGphone',
                            mimeType: 'textml',
                            uri: 'alipay://'
                        },function(ret,err){
                            var msg = JSON.stringify(ret);
                            
                        });
  }
  else
  {
  pay=0;
