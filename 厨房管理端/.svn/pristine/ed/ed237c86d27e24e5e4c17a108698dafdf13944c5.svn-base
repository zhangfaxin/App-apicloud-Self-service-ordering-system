<script type="text/javascript" src="../script/api.js"></script>
<script type="text/javascript">

var save=new Array([10]);
var menu=new Array([7]);
var order=new Array([7]);
var record=new Array([7]);
var storage=new Array([7]);
save[0]='save0';
save[1]='save1';
save[2]='save2';
save[3]='save3';
save[4]='save4';
save[5]='save5';
save[6]='save6';
save[7]='save7';
save[8]='save8';
save[9]='save9';
menu[0]="鸡汤氽海蚌";
menu[1]="诸葛烤鱼";
menu[2]="超级卤肉";
menu[3]="可乐鸡翅";
menu[4]="五香卤肉";
menu[5]="宫保鸡丁";
menu[6]="佛跳墙";	
storage[0]='0';
storage[1]='1';
storage[2]='2';
storage[3]='3';
storage[4]='4';
storage[5]='5';
storage[6]='6';
record[0]=0;
record[1]=0;
record[2]=0;
record[3]=0;
record[4]=0;
record[5]=0;
record[6]=0;
var jishu=0;
var caiming;
var detail;
var length=0;
var sign;
var shu;
    // 隐藏所有的一级frame
    apiready = function()
    {
		detail=api.pageParam.name;      
      var model=api.require('model');
      var query=api.require('query');
      query.createQuery({
      },function(ret,err){
      	//coding...
      	if(ret){
      	model.count({
	          class:'menu',
	          qid:ret.qid
          },function(ret,err){
          	//coding...
          	if(ret){
          	var count=ret.count;
          	model.findAll({
	              class:'menu',
	              qid:ret.qid
              },function(ret,err){
              	//coding...
              	if(ret){
              	for(var i=0;i<count;i++){
              	if(ret[i].table==detail){
              	var order=ret[i].order;
              	var sequence=ret[i].order;
              	var id=ret[i].id;
              	if(order.length>0){
				for(var h=0;h<order.length;h++){
						order[h]=sequence.substr(h,1);
						for(var j=0;j<7;j++){
								if(order[h]==j){
									record[j]=record[j]+1;
								}
						}
				}
			 	for(var k=0;k<7;k++){
						if(record[k]!=0){		
								document.getElementById(save[jishu]).innerHTML=menu[k]+":"+record[k]+"份";
								localStorage.setItem(storage[jishu],menu[k]);
								jishu++;
						}
				}	
				}
              	}
              	}
              	}
              });
          	}
          });
      	}
      });  
	}
	function inform(type){
	var model=api.require('model');
	var query=api.require('query');
	api.confirm({
	'title':'通知',
	'msg':'是否制作完成',
	'buttons':['是','否']
    },function(ret,err){
    	//coding...
    	if(ret.buttonIndex==1){
    	query.createQuery({
        },function(ret,err){
        	//coding...
        	if(ret){
        	model.count({
	            class:'menu',
	            qid:ret.qid
            },function(ret,err){
            	//coding...
            	if(ret){
            	var count=ret.count;
            	model.findAll({
	                class:'menu',
	                qid:ret.qid
                },function(ret,err){
                	//coding...
                	if(ret){
                	for(var i=0;i<7;i++){
                	if(save[i]==type){
                	shu=i;
                	break;
                	}
                	}
                	caiming=localStorage.getItem(storage[shu]);
                	for(var j=0;j<7;j++){
                	if(menu[j]==caiming){
                	sign=j;
                	break;
                	}
                	}
                	for(var i=0;i<count;i++){
                	if(ret[i].table==detail){
                	var id=ret[i].id;
                	model.updateById({
	                    class:'menu',
	                    id:id,
	                    value:{make:caiming}
                    },function(ret,err){
                    	//coding...
                    	if(ret){
                    	alert("已通知顾客");
                    	}
                    });
                	}
                	}
                	}
                	
                });
            	}
            });
        	}
        });
    	}else{
    	api.prompt({
    	'title':'其他问题',
    	'buttons':['确定','取消']
        },function(ret,err){
        	//coding...
        	if(ret.buttonIndex==1){
        	var message=ret.text;
    	query.createQuery({
        },function(ret,err){
        	//coding...
        	if(ret){
        	model.count({
	            class:'menu',
	            qid:ret.qid
            },function(ret,err){
            	//coding...
            	if(ret){
            	var count=ret.count;
            	model.findAll({
	                class:'menu',
	                qid:ret.qid
                },function(ret,err){
                	//coding...
                	if(ret){
                	for(var i=0;i<7;i++){
                	if(save[i]==type){
                	shu=i;
                	break;
                	}
                	}
					caiming=localStorage.getItem(storage[shu]);
                	for(var i=0;i<count;i++){
                	if(ret[i].table==detail){
                	var id=ret[i].id;
                	model.updateById({
	                    class:'menu',
	                    id:id,
	                    value:{message:caiming+":"+message}
                    },function(ret,err){
                    	//coding...
                    	if(ret){
                    	alert("已通知顾客");
                    	}
                    });
                	}
                	}

                	}
                	
                });
            	}
            });
        	}
        });        	
        	}
        	
        });
    	}
    });
	}

